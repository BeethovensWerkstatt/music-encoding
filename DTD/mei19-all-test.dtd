                     enclose.accid      %ENCLOSURE;           #IMPLIED
                     enclose.note       %ENCLOSURE;           #IMPLIED
                     headshape          %HEADSHAPE;           #IMPLIED
                     ho                 %VISUALOFFSET;        #IMPLIED
                     size               %SIZE;                #IMPLIED
