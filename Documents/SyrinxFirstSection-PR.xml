<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mei SYSTEM "../DTD/mei19-all-test.dtd"[
]>
<mei version="1.9b">
  <meihead>
    <filedesc>
      <titlestmt>
        <title>Syrinx (La Flûte de Pan)</title>
        <respstmt>
          <name/>
        </respstmt>
      </titlestmt>
      <pubstmt/>
    </filedesc>
    <profiledesc>
      <langusage>
        <language id="fr">French</language>
        <language id="it">Italian</language>
      </langusage>
    </profiledesc>
    <revisiondesc>
      <change>
        <changedesc>
          <p/>
        </changedesc>
        <date/>
      </change>
    </revisiondesc>
  </meihead>
  <music>
    <body>
      <mdiv>
        <score>
          <scoredef>
            <pghead1>
              <table>
                <tr>
                  <td>
                    <title>Syrinx<lb/>(La Flûte de Pan)</title>
                  </td>
                </tr>
                <tr>
                  <td>
                    <rend rend="right">
                      <name>Claude Debussy<lb/>(<date from="1862" to="1918"
                          >1862-1918</date>)</name>
                    </rend>
                  </td>
                </tr>
                <tr>
                  <td><title>
                      <rend fontstyle="ital" rend="left">Psyche</rend>
                    </title> (<name>Gabriel Mourey</name>)<lb/>III. Akt, 1. Szene:<lb/>«
                    Mais voici que Pan de sa flûte recommence à jouer... »</td>
                </tr>
              </table>
            </pghead1>
            <staffgrp>
              <staffdef n="1" id="P1" label.full="Flute" clef.line="2" clef.shape="G"/>
            </staffgrp>
          </scoredef>
          <section>
            <scoredef meter.count="3" meter.unit="4" key.sig="5f" key.mode="minor"/>
            <!-- PR: You could also encode the stage direction above in a <div> element. -->
            <div type="theatricalCue">
              <p>« Mais voici que Pan de sa flûte recommence à jouer... »</p>
            </div>
            <measure n="1" id="m1">
              <staff>
                <layer>
                  <beam>
                    <note pname="b" oct="5" dur="8" dots="1" stem.dir="down" beam="i1"
                      slur="i1" slur.dir="up" id="m1n1">
                      <artic value="ten" place="above"/>
                    </note>
                    <note pname="a" oct="5" dur="32" stem.dir="down" beam="m1" slur="m1"
                      id="m1n2">
                      <accid value="n"/>
                    </note>
                    <note pname="b" oct="5" dur="32" stem.dir="down" beam="f1" slur="t1"
                      id="m1n3">
                      <accid value="n"/>
                    </note>
                  </beam>
                  <note pname="a" oct="5" dur="8" dots="1" stem.dir="down" beam="i1"
                    slur="i1" id="m1n4">
                    <accid value="f"/>
                    <artic value="ten" place="above"/>
                  </note>
                  <note pname="g" oct="5" dur="32" stem.dir="down" beam="m1" slur="m1"
                    id="m1n5">
                    <accid value="n"/>
                  </note>
                  <note pname="a" oct="5" dur="32" stem.dir="down" beam="f1" slur="f1"
                    id="m1n6">
                    <accid value="n"/>
                  </note>
                  <note pname="g" oct="5" dur="16" stem.dir="down" beam="i1" slur="i1"
                    id="m1n7">
                    <accid value="f"/>
                  </note>
                  <note pname="f" oct="5" dur="16" stem.dir="down" beam="m1" slur="m1"
                    id="m1n8"/>
                  <note pname="e" oct="5" dur="16" stem.dir="down" beam="m1" slur="m1"
                    id="m1n9">
                    <accid value="n"/>
                  </note>
                  <note pname="d" oct="5" dur="16" stem.dir="down" beam="f1" slur="f1"
                    id="m1n10"/>
                </layer>
              </staff>
              <supplied>
                <dynam staff="1" tstamp="1">mf</dynam>
              </supplied>
              <dir place="above" staff="1" tstamp="0.5" lang="fr">
                <rend fontfam="Times" fontsize="10.25" fontstyle="normal">Très
                  modéré</rend>
              </dir>
            </measure>
            <measure n="2" id="m2">
              <staff>
                <layer>
                  <note pname="b" oct="5" dur="8" dots="1" stem.dir="down" beam="i1"
                    slur="i1" id="m2n1"/>
                  <note pname="c" oct="6" dur="32" stem.dir="down" beam="m1" slur="m1"
                    id="m2n2"/>
                  <note pname="c" oct="6" dur="32" stem.dir="down" beam="f1" slur="m1"
                    id="m2n3">
                    <accid value="f"/>
                  </note>
                  <note pname="b" oct="5" dur="2" stem.dir="down" slur="f1" id="m2n4"/>
                </layer>
              </staff>
              <fermata staff="1" place="above" tstamp="3" form="norm"/>
              <hairpin form="dim" place="below" staff="1" tstamp="1.25" dur="3"/>
              <breath staff="1" tstamp="3.9"/>
            </measure>
            <sb/>
            <measure n="3" id="m3">
              <staff>
                <layer>
                  <note pname="b" oct="5" dur="8" dots="1" stem.dir="down" beam="i1"
                    slur="i1" slur.dir="up" id="m3n1">
                    <artic value="ten" place="below"/>
                  </note>
                  <note pname="a" oct="5" dur="32" stem.dir="down" beam="m1" slur="m1"
                    id="m3n2">
                    <accid value="n"/>
                  </note>
                  <note pname="b" oct="5" dur="32" stem.dir="down" beam="f1" slur="f1"
                    id="m3n3">
                    <accid value="n"/>
                  </note>
                  <note pname="a" oct="5" dur="8" dots="1" stem.dir="down" beam="i1"
                    slur="i1" slur.dir="up" id="m3n4">
                    <accid value="f"/>
                    <artic value="ten" place="below"/>
                  </note>
                  <note pname="g" oct="5" dur="32" stem.dir="down" beam="m1" slur="m1"
                    id="m3n5">
                    <accid value="n"/>
                  </note>
                  <note pname="a" oct="5" dur="32" stem.dir="down" beam="f1" slur="f1"
                    id="m3n6">
                    <accid value="n"/>
                  </note>
                  <note pname="g" oct="5" dur="16" stem.dir="down" beam="i1" slur="i1"
                    slur.dir="up" id="m3n7">
                    <accid value="f"/>
                  </note>
                  <note pname="f" oct="5" dur="16" stem.dir="down" beam="m1" slur="m1"
                    id="m3n8"/>
                  <note pname="e" oct="5" dur="16" stem.dir="down" beam="m1" slur="m1"
                    id="m3n9">
                    <accid value="n"/>
                  </note>
                  <note pname="d" oct="5" dur="16" stem.dir="down" beam="f1" slur="f1"
                    id="m3n10"/>
                </layer>
              </staff>
            </measure>
            <measure n="4" id="m4">
              <staff>
                <layer>
                  <beam>
                    <note pname="b" oct="4" dur="8" dots="1" stem.dir="up" id="m4n1">
                      <artic value="ten" place="below"/>
                    </note>
                    <note pname="f" oct="4" dur="32" stem.dir="up" id="m4n2">
                      <accid value="s"/>
                    </note>
                    <note pname="g" oct="4" dur="32" stem.dir="up" beam="f1" id="m4n3">
                      <accid value="n"/>
                    </note>
                  </beam>
                  <beam>
                    <note pname="b" oct="4" dur="8" tie="i" stem.dir="up" id="m4n4">
                      <accid value="n"/>
                      <artic value="ten" place="below"/>
                    </note>
                    <tuplet>
                      <note pname="b" oct="4" dur="16" tie="t" stem.dir="up" id="m4n5"/>
                      <note pname="f" oct="4" dur="16" stem.dir="up" id="m4n6"/>
                      <note pname="g" oct="4" dur="16" stem.dir="up" id="m4n7"/>
                    </tuplet>
                  </beam>
                  <beam>
                    <tuplet>
                      <note pname="c" oct="5" dur="16" stem.dir="down" id="m4n8"/>
                      <note pname="f" oct="4" dur="16" stem.dir="down" id="m4n9"/>
                      <note pname="g" oct="4" dur="16" stem.dir="down" id="m4n10"/>
                    </tuplet>
                    <tuplet>
                      <note pname="d" oct="5" dur="16" stem.dir="down" id="m4n11"/>
                      <note grace="acc" pname="f" oct="5" dur="16" stem.dir="up"
                        id="m4n12"/>
                      <note pname="e" oct="5" dur="16" stem.dir="down" id="m4n13">
                        <accid value="n"/>
                      </note>
                      <note pname="d" oct="5" dur="16" stem.dir="down" id="m4n14"/>
                    </tuplet>
                  </beam>
                </layer>
              </staff>
              <tupletspan staff="1" startid="m4n5" endid="m4n7" num="3:2"
                num.visible="false" num.place="above" num.format="count"/>
              <tupletspan staff="1" startid="m4n8" endid="m4n10" num="3:2"
                num.visible="false" num.place="below" num.format="count"/>
              <tupletspan staff="1" startid="m4n11" endid="m4n13" num="3:2"
                num.visible="false" num.place="below" num.format="count"/>
              <hairpin tstamp="1" dur="2" form="cres" place="below" staff="1"/>
              <dynam tstamp="2" place="below" staff="1">p</dynam>
              <hairpin tstamp="2.6" dur="4" form="cres" place="below" staff="1"/>

              <!-- PR: Here's a simpler way to encode slurs. I admit a dir(ection) attribute might be
              useful here. -->
              <slur staff="1" tstamp="1" dur="1.125"/>
              <slur staff="1" tstamp="2.33" dur="4"/>
            </measure>
            <sb/>
            <measure n="5" id="m5">
              <staff>
                <layer>
                  <note pname="b" oct="4" dur="8" dots="1" stem.dir="up" beam="i1"
                    slur="m1" id="m5n1"/>
                  <note pname="f" oct="4" dur="32" stem.dir="up" beam="m1" slur="m1"
                    id="m5n2">
                    <accid value="s"/>
                  </note>
                  <note pname="g" oct="4" dur="32" stem.dir="up" beam="f1" slur="m1"
                    id="m5n3">
                    <accid value="n"/>
                  </note>
                  <note pname="b" oct="4" dur="4" stem.dir="down" slur="f1" id="m5n4"
                    corresp="m5n4a">
                    <accid value="n"/>
                  </note>
                  <supplied>
                    <note pname="c" oct="4" stem.dir="down" id="m5n4a" corresp="m5n4"
                      enclose.note="paren" pname.ges="c" accid.ges="f">
                      <accid value="f"/>
                    </note>
                  </supplied>
                  <rest id="m5r1" dur="16" tuplet="i1" beam="i1"/>
                  <note pname="a" oct="4" dur="16" tuplet="m1" stem.dir="down" beam="m1"
                    slur="i1" slur.dir="up" id="m5n5"/>
                  <note pname="c" oct="5" dur="16" tuplet="t1" stem.dir="down" beam="m1"
                    slur="m1" id="m5n6">
                    <accid value="f"/>
                  </note>
                  <note pname="d" oct="5" dur="16" tuplet="i1" stem.dir="down" beam="m1"
                    slur="m1" id="m5n7"/>
                  <note pname="f" oct="5" dur="16" tuplet="m1" stem.dir="down" beam="m1"
                    slur="m1" id="m5n8">
                    <accid value="f"/>
                  </note>
                  <note pname="a" oct="5" dur="16" tuplet="t1" stem.dir="down" beam="m1"
                    slur="f1" id="m5n9"/>
                </layer>
              </staff>
              <tupletspan staff="1" startid="m5n5" endid="m5n7" num="3:2"
                num.visible="false" num.place="below" num.format="count"
                bracket.place="below" bracket.visible="false"/>
              <tupletspan staff="1" startid="m5n7" endid="m5n9" num="3:2"
                num.visible="false" num.place="below" num.format="count"/>
              <dynam place="below" staff="1">p</dynam>
              <hairpin tstamp="1.75" dur="2" form="cres" place="below" staff="1"/>
              <hairpin tstamp="3.16" dur="3.5" form="cres" place="below" staff="1"/>
            </measure>

            <measure n="6" id="m6">
              <staff>
                <layer>
                  <rest id="m6r1" dur="8" dots="1"/>
                  <note pname="a" oct="5" dur="16" stem.dir="down" id="m6n1"/>
                  <note pname="c" oct="6" dur="2" tie="i" stem.dir="down" id="m6n2">
                    <accid value="f"/>
                  </note>
                </layer>
              </staff>
              <hairpin staff="1" tstamp="2" dur="1m+1.5" form="cres" place="below"/>
              <!-- Raffaele: @corresp should be @sameAs (one item represented twice as opposed to
                being two different representations of the same thing)-->
              <dynam place="below" staff="1">mf</dynam>
            </measure>
            <measure n="7" id="m7">
              <staff>
                <layer>
                  <note pname="c" oct="6" dur="2" tie="t" stem.dir="down" slur="i1"
                    slur.dir="up" id="m7n1"/>
                  <note pname="d" oct="6" dur="4" stem.dir="down" slur="f1" id="m7n2"/>
                </layer>
              </staff>
              <hairpin form="dim" place="below" staff="1" tstamp="3" dur="4"/>
            </measure>
            <measure n="8" right="dbl" id="m8">
              <staff>
                <layer>
                  <note pname="e" oct="6" dur="2" stem.dir="down" slur="i1" slur.dir="up"
                    id="m8n1"/>
                  <note pname="b" oct="5" dur="4" stem.dir="down" slur="f1" id="m8n2"/>
                </layer>
              </staff>
              <dynam place="below" staff="1">p</dynam>
              <hairpin form="dim" place="below" staff="1" tstamp="2" dur="3.75"/>

              <!-- supplied breath -->
              <supplied>
                <dir staff="1" tstamp="3.85">,</dir>
              </supplied>

              <!-- PR: Generally, the continuation line should be generated by the renderer, not encoded
              as part of the directive text. That way, different layouts will generate a line of the correct
              length.  Therefore, use <dir dur="3.5"> to get a line until the 2nd half of beat 3. -->
              <dir place="above" staff="1" lang="fr" dur="3.5">
                <rend fontfam="Times" fontsize="10.25" fontstyle="normal">Retenu</rend>
              </dir>
            </measure>
          </section>
          <div>
            <p>« Tais-toi, contiens ta joie, écoute. »</p>
          </div>
          <!-- new section ... -->
        </score>
      </mdiv>
    </body>
  </music>
</mei>
