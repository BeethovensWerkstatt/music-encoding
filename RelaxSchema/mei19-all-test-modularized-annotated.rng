    <ref name="att.edit"/>
    <ref name="att.facsimile"/>
    <ref name="att.lang"/>
    <ref name="att.trans"/>
    <ref name="att.typed"/>
    <optional>
      <attribute name="expan"/>
    </optional>
  </define>
