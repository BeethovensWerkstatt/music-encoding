  <div xml:id="att.anl.mrpt2">
    <xhtml:div>
      <xhtml:h1 class="name">att.anl.mrpt2</xhtml:h1>
      <xhtml:div class="desc">
        <xhtml:p>Analytical domain attributes: mrpt2 uses on common analysis attributes.</xhtml:p>
      </xhtml:div>
    </xhtml:div>
    <define name="att.anl.mrpt2">
