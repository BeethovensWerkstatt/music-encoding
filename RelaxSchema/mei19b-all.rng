      <xhtml:div>
        <xhtml:h1 class="name">content.measure</xhtml:h1>
        <xhtml:div class="desc"/>
      </xhtml:div>
      <define name="content.measure">
        <zeroOrMore>
          <ref name="model.Incl"/>
        </zeroOrMore>
        <zeroOrMore>
          <ref name="model.measurePart"/>
          <zeroOrMore>
            <ref name="model.Incl"/>
          </zeroOrMore>
        </zeroOrMore>
      </define>
    </div>
