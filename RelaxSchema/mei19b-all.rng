      </define>
    </div>
    <div xml:id="attlist.handshift">
      <xhtml:div>
        <xhtml:h1 class="name">attlist.handshift</xhtml:h1>
        <xhtml:div class="desc"/>
      </xhtml:div>
      <define name="attlist.handshift" combine="interleave">
        <ref name="att.common"/>
        <ref name="att.edit"/>
        <ref name="att.facsimile"/>
        <optional>
          <attribute name="character"/>
        </optional>
        <optional>
          <attribute name="medium"/>
        </optional>
        <optional>
          <attribute name="new">
            <data type="IDREF"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="old">
            <data type="IDREF"/>
          </attribute>
        </optional>
      </define>
    </div>
  </div>

