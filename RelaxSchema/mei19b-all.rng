        <optional>
          <attribute name="halign">
            <choice>
              <value>left</value>
              <value>right</value>
              <value>center</value>
              <value>justify</value>
            </choice>
          </attribute>
        </optional>
        <optional>
          <attribute name="rend">
            <ref name="data.TEXTRENDITION"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="rotation">
            <ref name="data.DEGREES"/>
          </attribute>
        </optional>
        <optional>
          <attribute name="valign">
            <choice>
              <value>top</value>
              <value>middle</value>
              <value>bottom</value>
              <value>baseline</value>
            </choice>
          </attribute>
        </optional>
      </define>
    </div>
  </div>

