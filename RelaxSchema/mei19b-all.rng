        <oneOrMore>
          <ref name="edition"/>
          <zeroOrMore>
            <ref name="respstmt"/>
          </zeroOrMore>
        </oneOrMore>
      </define>
    </div>
    <div xml:id="attlist.editionstmt">
      <xhtml:div>
        <xhtml:h1 class="name">attlist.editionstmt</xhtml:h1>
        <xhtml:div class="desc"/>
      </xhtml:div>
      <define name="attlist.editionstmt" combine="interleave">
