        <ref name="att.edit"/>
        <ref name="att.facsimile"/>
        <optional>
          <attribute name="agent"/>
        </optional>
        <optional>
          <attribute name="reason"/>
        </optional>
      </define>
    </div>
  </div>

  <div xml:id="titlepage">
    <xhtml:div>
      <xhtml:h1 class="name">titlepage</xhtml:h1>
      <xhtml:div class="desc">
        <xhtml:p>The titlepage element contains a transcription of the title page of a text. It may
          be used within the physdesc element when no other transcription is provided. Used by:
          model.frontPart model.physdescPart Module: meiEditTrans</xhtml:p>
      </xhtml:div>
