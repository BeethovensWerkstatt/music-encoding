  <div xml:id="att.log.harppedal">
    <xhtml:div>
      <xhtml:h1 class="name">att.log.harppedal</xhtml:h1>
      <xhtml:div class="desc">
        <xhtml:p>Logical domain attributes: The pedal setting, flat, natural, or sharp, for each
          diatonic pitch name is indicated by the seven letter- named attributes.</xhtml:p>
      </xhtml:div>
    </xhtml:div>
    <define name="att.log.harppedal">
      <ref name="att.controlevent"/>
      <ref name="att.startendid"/>
      <optional>
        <attribute name="c" a:defaultValue="n">
          <choice>
            <value>f</value>
            <value>n</value>
            <value>s</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="d" a:defaultValue="n">
          <choice>
            <value>f</value>
            <value>n</value>
            <value>s</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="e" a:defaultValue="n">
          <choice>
            <value>f</value>
            <value>n</value>
            <value>s</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="f" a:defaultValue="n">
          <choice>
            <value>f</value>
            <value>n</value>
            <value>s</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="g" a:defaultValue="n">
          <choice>
            <value>f</value>
            <value>n</value>
            <value>s</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="a" a:defaultValue="n">
          <choice>
            <value>f</value>
            <value>n</value>
            <value>s</value>
          </choice>
        </attribute>
      </optional>
      <optional>
        <attribute name="b" a:defaultValue="n">
          <choice>
            <value>f</value>
            <value>n</value>
            <value>s</value>
          </choice>
        </attribute>
      </optional>
    </define>
  </div>

