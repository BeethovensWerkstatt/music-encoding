        <ref name="att.facsimile"/>
        <ref name="att.trans"/>
        <ref name="att.typed"/>
        <optional>
          <attribute name="desc"/>
        </optional>
      </define>
    </div>
  </div>

  <div xml:id="sic">
    <xhtml:div>
      <xhtml:h1 class="name">sic</xhtml:h1>
      <xhtml:div class="desc">
        <xhtml:p>Contains apparently incorrect or inaccurate musical material. A correction for the
          apparent error may be given in an accompanying child or sibling corr element. Used by:
          model.transcriptionLike model.choicePart Module: meiEditTrans</xhtml:p>
