<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" targetNamespace="http://www.w3.org/XML/1998/namespace">
  <xs:import schemaLocation="mei19-all.xsd"/>
  <xs:attribute name="id" type="xs:ID">
    <xs:annotation>
      <xs:documentation>regularizes the naming of an element and thus
          facilitates building links between it and other resources. Each id attribute
          within a document must have a unique value.</xs:documentation>
    </xs:annotation>
  </xs:attribute>
</xs:schema>
