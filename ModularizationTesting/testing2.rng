  </define>
  <define name="chord">
    <element name="chord">
      <a:documentation xml:lang="eng">chord ― A simultaneous sounding of 2 or
        more notes in the same layer *with the same duration*. See Read, p.
        69.</a:documentation>
      <ref name="attlist.chord"/>
      <ref name="content.chord"/>
    </element>
  </define>
  <define name="attlist.chord">
    <ref name="att.common"/>
    <ref name="att.facsimile"/>
  </define>

  <!-- CMN module -->
  <define name="model.sectionPart" combine="interleave">
    <a:documentation xml:lang="eng">groups elements that may appear as part of a
      section.</a:documentation>
    <zeroOrMore>
      <ref name="measure"/>
    </zeroOrMore>
  </define>

  <define name="model.measurePart">
    <a:documentation xml:lang="eng">groups elements that may appear as part of a
      measure.</a:documentation>
    <choice>
      <ref name="staff"/>
    </choice>
  </define>

  <define name="model.layerPart" combine="interleave">
    <a:documentation xml:lang="eng">groups notated events at the layer
      level.</a:documentation>
    <zeroOrMore>
      <choice>
        <ref name="barline"/>
        <ref name="note"/>
        <ref name="rest"/>
        <ref name="chord"/>
        <ref name="clefchange"/>
        <ref name="custos"/>
        <ref name="pad"/>
      </choice>
    </zeroOrMore>
  </define>

  <define name="content.measure">
    <zeroOrMore>
      <choice>
        <ref name="model.measurePart"/>
      </choice>
    </zeroOrMore>
  </define>
  <define name="measure">
    <element name="measure">
      <a:documentation xml:lang="eng">measure ― "In Western notation a vertical
        line drawn through the staff to mark off metrical units. Hence also the
        metrical unit thus indicated, which in American usage is called
        'measure'. English usage often relies on context alone to make the
        distinction clear (e.g. 'up to the double bar', 'the end of the bar'),
        but 'bar-line' is also common." (New Grove) In MEI, the measure element
        is also a linking element that uses attributes to connect the MEI
        document to an external electronic object or to another location within
        the MEI instance.</a:documentation>
      <ref name="content.measure"/>
    </element>
  </define>

  <!-- Mensural module
  <define name="model.sectionPart" combine="interleave">
    <a:documentation xml:lang="eng">groups elements that may appear as part of a
      section.</a:documentation>
    <zeroOrMore>
      <ref name="staff"/>
    </zeroOrMore>
    </define> -->

  <!-- Neumes module
  <define name="model.sectionPart" combine="interleave">
    <a:documentation xml:lang="eng">groups elements that may appear as part of a
      section.</a:documentation>
    <zeroOrMore>
      <ref name="staff"/>
    </zeroOrMore>
    </define> -->

  <define name="model.staffPart" combine="interleave">
    <a:documentation xml:lang="eng">groups elements that may appear as part of
      the content of a staff.</a:documentation>
    <zeroOrMore>
      <ref name="syllable"/>
    </zeroOrMore>
  </define>

  <define name="model.syllablePart">
    <a:documentation xml:lang="eng">groups elements that may appear as part of
      content of a syllable.</a:documentation>
    <zeroOrMore>
      <choice>
        <ref name="syl"/>
        <ref name="ineume"/>
        <ref name="uneume"/>
      </choice>
    </zeroOrMore>
  </define>

  <define name="content.syllable">
    <zeroOrMore>
      <choice>
        <ref name="model.syllablePart"/>
      </choice>
    </zeroOrMore>
  </define>
  <define name="syllable">
    <element name="syllable">
      <a:documentation xml:lang="eng">syllable ― Neume notation can be thought
        of as "neumed text". Therefore, the syllable element provides high-level
        organization in this repertoire.</a:documentation>
      <ref name="content.syllable"/>
    </element>
  </define>

  <define name="content.syl">
    <zeroOrMore>
      <choice>
        <text/>
      </choice>
    </zeroOrMore>
  </define>
  <define name="syl">
    <element name="syl">
      <a:documentation xml:lang="eng">syllable ― Individual lyric syllable. Do
        not confuse this element with the syllable element.</a:documentation>
      <ref name="content.syl"/>
    </element>
  </define>

  <define name="content.ineume">
    <empty/>
  </define>
  <define name="ineume">
    <element name="ineume">
      <a:documentation xml:lang="eng">interrupted neume ― </a:documentation>
      <ref name="attlist.ineume"/>
      <ref name="content.ineume"/>
    </element>
  </define>
  <define name="attlist.ineume">
    <ref name="att.common"/>
    <ref name="att.facsimile"/>
    <ref name="att.typed"/>
  </define>
  
  <define name="content.uneume">
    <empty/>
  </define>
  <define name="uneume">
    <element name="uneume">
      <a:documentation xml:lang="eng">uninterrupted neume ― A single,
        uninterrupted neume sign. While ineume is allowed as a sub-element of
        uneume here as part of the model.eventLike.neume class, it is unlikely
        that an uninterrupted neume containing an interrupted neume is a
        meaningful construct.</a:documentation>
      <ref name="attlist.uneume"/>
      <ref name="content.uneume"/>
    </element>
  </define>
  <define name="attlist.uneume">
    <ref name="att.common"/>
    <ref name="att.facsimile"/>
    <ref name="att.typed"/>
  </define>

  <!-- Facsimile module -->
  <define name="model.facsimileLike" combine="interleave">
    <ref name="facsimile"/>
  </define>

  <define name="content.facsimile">
    <zeroOrMore>
      <choice>
        <ref name="graphic"/>
        <ref name="surface"/>
      </choice>
    </zeroOrMore>
  </define>
  <define name="facsimile">
    <element name="facsimile">
      <a:documentation xml:lang="eng">facsimile ― Contains a representation of
        some written source in the form of a set of images rather than as
        transcribed or encoded text. The source attribute may be used to link
        the collection of images with a particular source.</a:documentation>
      <ref name="attlist.facsimile"/>
      <ref name="content.facsimile"/>
    </element>
  </define>
  <define name="attlist.facsimile">
    <ref name="att.common"/>
    <optional>
      <attribute name="source">
        <a:documentation xml:lang="eng">A source description in the header may
          be pointed to by putting its ID in the source
          attribute.</a:documentation>
        <data type="IDREF"/>
      </attribute>
    </optional>
  </define>

  <define name="content.surface">
    <zeroOrMore>
      <ref name="graphic"/>
    </zeroOrMore>
  </define>
  <define name="surface">
    <element name="surface">
      <a:documentation xml:lang="eng">surface ― Defines a written surface in
        terms of a rectangular coordinate space, optionally grouping one or more
        graphic representations of that space, and rectangular zones of interest
        within it.</a:documentation>
      <ref name="attlist.surface"/>
      <ref name="content.surface"/>
    </element>
  </define>
  <define name="attlist.surface">
    <ref name="att.common"/>
    <optional>
      <attribute name="startid">
        <a:documentation xml:lang="eng">holds a reference to the first feature
          occurring on this surface.</a:documentation>
        <data type="IDREF"/>
      </attribute>
    </optional>
  </define>

  <!-- Header module -->
  <define name="model.availabilityPart">
    <a:documentation xml:lang="eng">groups elements that may appear as part of a
      description of the availability of and access to a bibliographic
      item.</a:documentation>
    <zeroOrMore>
      <ref name="acqsource"/>
      <zeroOrMore>
        <ref name="accessdesc"/>
        <optional>
          <ref name="price"/>
        </optional>
      </zeroOrMore>
    </zeroOrMore>
    <optional>
      <ref name="userestrict"/>
    </optional>
    <optional>
      <ref name="sysreq"/>
    </optional>
  </define>

  <define name="model.editorialdeclPart">
    <a:documentation xml:lang="eng">groups elements that may appear as part of a
      description of the editorial process applied to the encoding of
      notation.</a:documentation>
    <choice>
      <ref name="correction"/>
      <ref name="interpretation"/>
      <ref name="normalization"/>
      <ref name="segmentation"/>
      <ref name="stdvals"/>
    </choice>
  </define>

  <define name="model.encodingPart">
    <a:documentation xml:lang="eng">groups elements that may appear as part of
      information regarding the encoding process.</a:documentation>
    <optional>
      <ref name="appinfo"/>
    </optional>
    <optional>
      <ref name="projectdesc"/>
    </optional>
    <optional>
      <ref name="samplingdecl"/>
    </optional>
    <optional>
      <ref name="editorialdecl"/>
    </optional>
  </define>

  <define name="model.headerPart">
    <a:documentation xml:lang="eng">groups elements that may appear as part of
      the MEI header.</a:documentation>
    <optional>
      <ref name="encodingdesc"/>
    </optional>
    <optional>
      <ref name="profiledesc"/>
    </optional>
  </define>

  <define name="model.physdescPart">
    <a:documentation xml:lang="eng">groups elements that may appear as part of
      the physical description of a bibliographic item.</a:documentation>
    <choice>
      <ref name="condition"/>
      <ref name="dimensions"/>
      <ref name="exhibithist"/>
      <ref name="extent"/>
      <ref name="handlist"/>
      <ref name="inscription"/>
      <ref name="physmedium"/>
      <ref name="physloc"/>
      <ref name="provenance"/>
      <ref name="repository"/>
      <ref name="treatmenthist"/>
      <ref name="treatmentsched"/>
    </choice>
  </define>

  <define name="model.profiledescPart">
    <a:documentation xml:lang="eng">groups elements that may appear as part of
      the profile description.</a:documentation>
    <optional>
      <ref name="creation"/>
    </optional>
    <optional>
      <ref name="langusage"/>
    </optional>
    <optional>
      <ref name="classification"/>
    </optional>
  </define>

  <define name="model.pubstmtPart">
    <a:documentation xml:lang="eng">groups elements that may appear as part of
      the publication statement for a bibliographic item.</a:documentation>
    <choice>
      <ref name="address"/>
      <ref name="respstmt"/>
      <ref name="date"/>
      <ref name="identifier"/>
      <ref name="availability"/>
    </choice>
  </define>

  <define name="meihead">
    <element name="meihead">
      <a:documentation xml:lang="eng">meihead ― A wrapper for general
        bibliographic and descriptive information about the encoded file. This
        information is often essential in a machine-readable environment. Five
        sub-elements must be encoded in the following order:
        &lt;altmeiid&gt;(optional), &lt;filedesc&gt;(required),
        &lt;encodingdesc&gt;(optional),
        &lt;profiledesc;&gt;(optional), and
        &lt;revisiondesc&gt;(optional). These elements and their
        sub-elements provide: a unique identifier for the MEI file,
        bibliographic information about the MEI file and its sources,
        information about the encoding process, information about the creation
        of the work being encoded, and statements regarding significant
        revisions of the file.</a:documentation>
      <ref name="attlist.meihead"/>
      <zeroOrMore>
        <ref name="altmeiid"/>
      </zeroOrMore>
      <ref name="filedesc"/>
      <ref name="model.headerPart"/>
      <optional>
        <ref name="revisiondesc"/>
      </optional>
    </element>
  </define>
  <define name="attlist.meihead">
    <ref name="att.bibl"/>
    <ref name="att.common"/>
    <ref name="att.lang"/>
    <optional>
      <attribute name="type">
        <a:documentation xml:lang="eng">specifies the kind of document to which
          the header is attached, that is, whether it is a corpus or an
          individual text.</a:documentation>
        <choice>
          <value>music</value>
          <value>corpus</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="version" a:defaultValue="1.9b">
        <ref name="meiVersion"/>
      </attribute>
    </optional>
  </define>

  <define name="content.accessdesc">
    <zeroOrMore>
      <choice>
        <text/>
      </choice>
    </zeroOrMore>
  </define>
  <define name="accessdesc">
    <element name="accessdesc">
      <a:documentation xml:lang="eng">access description ― Describes the
        conditions that affect the accessibility of a bibliographic item. May
        indicate the nature of restrictions or the lack of
        restrictions.</a:documentation>
      <ref name="attlist.accessdesc"/>
      <ref name="content.accessdesc"/>
    </element>
  </define>
  <define name="attlist.accessdesc">
    <ref name="att.common"/>
    <ref name="att.bibl"/>
  </define>

  <define name="content.acqsource">
    <zeroOrMore>
      <choice>
        <text/>
      </choice>
    </zeroOrMore>
  </define>
  <define name="acqsource">
    <element name="acqsource">
      <a:documentation xml:lang="eng">acquisition source ― Post-publication
        source from which access to a bibliographic item may be obtained, e.g.
        vendor, distributor, etc.</a:documentation>
      <ref name="attlist.acqsource"/>
      <ref name="content.acqsource"/>
    </element>
  </define>
  <define name="attlist.acqsource">
    <ref name="att.common"/>
    <ref name="att.bibl"/>
  </define>

  <define name="content.altmeiid">
    <zeroOrMore>
      <choice>
        <text/>
      </choice>
    </zeroOrMore>
  </define>
  <define name="altmeiid">
    <element name="altmeiid">
      <a:documentation xml:lang="eng">alternative MEI id ― May contain a
        bibliographic identifier that does not fit within the meihead id
        attribute, for example because the id does not fit the definition of an
        XML id or because multiple identifiers are needed. One or the other of
        &lt;altmeiid&gt; or the id attribute on &lt;mei&gt; is
