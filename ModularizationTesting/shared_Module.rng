  <define name="att.vis.staffdef" combine="interleave">
    <a:documentation xml:lang="eng">Visual domain attributes for staffdef in the
      CMN repertoire</a:documentation>
    <!-- <optional>
      <attribute name="beam.rend">
        <a:documentation xml:lang="eng">encodes whether a beam is feathered and
