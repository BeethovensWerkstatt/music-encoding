        <choice>
          <value>line</value>
          <value>term</value>
        </choice>
      </attribute>
    </optional>
    <optional>
      <attribute name="reh.enclose">
        <a:documentation xml:lang="eng">describes the enclosing shape for
          rehearsal marks.</a:documentation>
        <choice>
          <value>box</value>
          <value>circle</value>
          <value>none</value>
        </choice>
      </attribute>
    </optional> -->
    <optional>
      <attribute name="scale">
        <a:documentation xml:lang="eng">indicates how the staff should be scaled
          when rendered.</a:documentation>
        <ref name="data.NUMBER"/>
      </attribute>
    </optional>
    <!-- <optional>
      <attribute name="slur.rend">
        <a:documentation xml:lang="eng">describes the default rendering style
          for slurs.</a:documentation>
        <ref name="data.CURVERENDITION"/>
      </attribute>
    </optional> -->
    <optional>
      <attribute name="spacing">
        <a:documentation xml:lang="eng">records the absolute distance (as
          opposed to the relative distances recorded in &lt;scoredef&gt;
          elements) between this staff and the preceding one in the same system.
          This value is meaningless for the first staff in a system since the
          spacing.system attribute indicates the spacing between
          systems.</a:documentation>
        <ref name="data.NUMBER"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="text.dist">
        <a:documentation xml:lang="eng">determines how far from the staff to
          render text elements.</a:documentation>
        <ref name="data.NUMBER"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="text.fam">
        <a:documentation xml:lang="eng">provides a default value for the font
          family name for text (other than lyrics) associated with this
          staff.</a:documentation>
        <ref name="data.FONTFAMILY"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="text.name">
        <a:documentation xml:lang="eng">provides a default value for the font
          name for text (other than lyrics) associated with this
          staff.</a:documentation>
        <ref name="data.FONTNAME"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="text.size">
