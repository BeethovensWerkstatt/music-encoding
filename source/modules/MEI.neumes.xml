<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_odds.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<div xmlns="http://www.tei-c.org/ns/1.0"
     xmlns:tei="http://www.tei-c.org/ns/1.0"
     xmlns:xi="http://www.w3.org/2001/XInclude">
   <specGrp xml:id="module.MEI.neumes">
      <moduleSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                  xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                  xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                  ident="MEI.neumes"
                  mode="add"
                  predeclare="false">
         <desc>Neume repertoire component declarations.</desc>
      </moduleSpec>
      <macroSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="macro.neumeModifierLike"
                 module="MEI.neumes"
                 type="pe"
                 mode="add"
                 predeclare="false">
         <desc>Groups elements that modify neume-like features.</desc>
         <content autoPrefix="true">
            <rng:empty/>
         </content>
      </macroSpec>
      <macroSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="data.STAFFITEM.neumes"
                 module="MEI.neumes"
                 type="dt"
                 mode="add"
                 predeclare="false">
         <desc>Items in the Neume repertoire that may be printed near a staff.</desc>
      </macroSpec>
      <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="att.nc.log"
                 module="MEI.neumes"
                 type="atts"
                 mode="add"
                 predeclare="false">
         <desc>Logical domain attributes.</desc>
         <classes mode="replace">
            <memberOf key="att.pitched" mode="add"/>
         </classes>
      </classSpec>
      <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="att.neume.log"
                 module="MEI.neumes"
                 type="atts"
                 mode="add"
                 predeclare="false">
         <desc>Logical domain attributes.</desc>
         <classes mode="replace">
            <memberOf key="att.event" mode="add"/>
            <memberOf key="att.sylText" mode="add"/>
         </classes>
      </classSpec>
      <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="att.syllable.log"
                 module="MEI.neumes"
                 type="atts"
                 mode="add"
                 predeclare="false">
         <desc>Logical domain attributes.</desc>
         <classes mode="replace">
            <memberOf key="att.alignment" mode="add"/>
         </classes>
      </classSpec>
      <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="model.eventLike.neumes"
                 module="MEI.neumes"
                 type="model"
                 mode="add"
                 predeclare="false">
         <desc>Groups event elements that occur in the neume repertoire.</desc>
         <classes mode="replace">
            <memberOf key="model.layerPart.neumes" mode="add"/>
            <memberOf key="model.syllablePart" mode="add"/>
         </classes>
      </classSpec>
      <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="model.layerPart.neumes"
                 module="MEI.neumes"
                 type="model"
                 mode="add"
                 predeclare="false">
         <desc>Groups notated events that may appear at the layer level in the neume repertoire.</desc>
         <classes mode="replace">
            <memberOf key="model.layerPart.mensuralAndNeumes" mode="add"/>
         </classes>
      </classSpec>
      <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="model.scorePart.neumes"
                 module="MEI.neumes"
                 type="model"
                 mode="add"
                 predeclare="false">
         <desc>Groups elements that may appear as part of a score in the neume repertoire.</desc>
      </classSpec>
      <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="model.sectionPart.neumes"
                 module="MEI.neumes"
                 type="model"
                 mode="add"
                 predeclare="false">
         <desc>Groups elements that may appear as part of a section in the neume repertoire.</desc>
         <classes mode="replace">
            <memberOf key="model.sectionPart.mensuralAndNeumes" mode="add"/>
         </classes>
      </classSpec>
      <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="model.staffPart.neumes"
                 module="MEI.neumes"
                 type="model"
                 mode="add"
                 predeclare="false">
         <desc>Groups elements that are components of a staff in the neume repertoire.</desc>
         <classes mode="replace">
            <memberOf key="model.staffPart.mensuralAndNeumes" mode="add"/>
         </classes>
      </classSpec>
      <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="model.syllableLike"
                 module="MEI.neumes"
                 type="model"
                 mode="add"
                 predeclare="false">
         <desc>Groups elements that accommodate neumed text.</desc>
         <classes mode="replace">
            <memberOf key="model.layerPart.neumes" mode="add"/>
         </classes>
      </classSpec>
      <classSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                 xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                 xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                 ident="model.syllablePart"
                 module="MEI.neumes"
                 type="model"
                 mode="add"
                 predeclare="false">
         <desc>Groups elements that may appear as part of the content of a syllable.</desc>
      </classSpec>
      <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                   ident="nc"
                   module="MEI.neumes"
                   mode="add"
                   predeclare="false"
                   ns="http://www.tei-c.org/ns/1.0">
         <desc>Sign representing a single pitched event, although the exact pitch may not be
        known.</desc>
         <classes mode="replace">
            <memberOf key="att.common" mode="add"/>
            <memberOf key="att.facsimile" mode="add"/>
            <memberOf key="att.nc.log" mode="add"/>
            <memberOf key="att.nc.vis" mode="add"/>
            <memberOf key="att.nc.ges" mode="add"/>
            <memberOf key="att.nc.anl" mode="add"/>
         </classes>
         <content autoPrefix="true">
            <rng:empty/>
         </content>
      </elementSpec>
      <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                   ident="neume"
                   module="MEI.neumes"
                   mode="add"
                   predeclare="false"
                   ns="http://www.tei-c.org/ns/1.0">
         <desc>Sign representing one or more musical pitches.</desc>
         <classes mode="replace">
            <memberOf key="att.common" mode="add"/>
            <memberOf key="att.facsimile" mode="add"/>
            <memberOf key="att.neume.log" mode="add"/>
            <memberOf key="att.neume.vis" mode="add"/>
            <memberOf key="att.neume.ges" mode="add"/>
            <memberOf key="att.neume.anl" mode="add"/>
            <memberOf key="model.eventLike.neumes" mode="add"/>
         </classes>
         <content autoPrefix="true">
            <rng:zeroOrMore>
               <rng:choice>
                  <rng:ref name="nc"/>
                  <rng:ref name="macro.neumeModifierLike"/>
                  <rng:ref name="model.appLike"/>
                  <rng:ref name="model.editLike"/>
                  <rng:ref name="model.transcriptionLike"/>
                  <rng:ref name="model.verseLike"/>
               </rng:choice>
            </rng:zeroOrMore>
         </content>
      </elementSpec>
      <elementSpec xmlns:rng="http://relaxng.org/ns/structure/1.0"
                   xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                   xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0"
                   ident="syllable"
                   module="MEI.neumes"
                   mode="add"
                   predeclare="false"
                   ns="http://www.tei-c.org/ns/1.0">
         <desc>Neume notation can be thought of as "neumed text". Therefore, the syllable element
        provides high-level organization in this repertoire.</desc>
         <classes mode="replace">
            <memberOf key="att.common" mode="add"/>
            <memberOf key="att.syllable.anl" mode="add"/>
            <memberOf key="att.syllable.ges" mode="add"/>
            <memberOf key="att.syllable.log" mode="add"/>
            <memberOf key="att.syllable.vis" mode="add"/>
            <memberOf key="model.syllableLike" mode="add"/>
         </classes>
         <content autoPrefix="true">
            <rng:zeroOrMore>
               <rng:choice>
                  <rng:ref name="model.annotLike"/>
                  <rng:ref name="model.appLike"/>
                  <rng:ref name="model.divLike"/>
                  <rng:ref name="model.editLike"/>
                  <rng:ref name="model.graphicPrimitiveLike"/>
                  <rng:ref name="model.milestoneLike.music"/>
                  <rng:ref name="model.noteModifierLike"/>
                  <rng:ref name="model.scoreDefLike"/>
                  <rng:ref name="model.staffDefLike"/>
                  <rng:ref name="model.staffGrpLike"/>
                  <rng:ref name="model.syllablePart"/>
                  <rng:ref name="model.transcriptionLike"/>
               </rng:choice>
            </rng:zeroOrMore>
         </content>
      </elementSpec>
   </specGrp>
</div>
