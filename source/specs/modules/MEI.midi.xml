         <content autoPrefix="true">
            <rng:empty/>
         </content>
         <attList org="group">
            <attDef ident="form"
                    usage="req"
                    mode="add"
                    predeclare="false"
                    ns="http://www.tei-c.org/ns/1.0">
               <desc>Indicates whether this is note-on or note-off velocity data.</desc>
               <valList type="closed" mode="add">
                  <valItem ident="on" mode="add">
                     <desc>Note-on velocity.</desc>
                  </valItem>
                  <valItem ident="off" mode="add">
                     <desc>Note-off velocity.</desc>
                  </valItem>
               </valList>
            </attDef>
         </attList>
      </elementSpec>
   </specGrp>
