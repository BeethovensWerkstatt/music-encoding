<?oxygen RNGSchema="../RelaxSchema/mei19b-all.rng" type="xml"?>
<mei version="1.9b">
  <meihead>
    <filedesc>
      <titlestmt>
        <title>Giga, Presto</title>
        <respstmt>
          <name n="composer">Vivaldi</name>
        </respstmt>
      </titlestmt>
      <pubstmt/>
    </filedesc>
    <profiledesc>
      <langusage>
        <language id="it"/>
      </langusage>
    </profiledesc>
  </meihead>
  <music>
    <body>
      <mdiv>
        <score>
          <scoredef key.sig="3f" meter.count="4" meter.unit="4" meter.sym="common">
            <staffgrp barthru="true" symbol="bracket">
              <!-- staff 1 *displays* a different time sig. Use proport.num and .numbase
              to give a proportion relative to the meter in scoredef? -->
              <staffdef n="1" clef.line="2" clef.shape="G" meter.count="12" meter.unit="8"
                proport.num="3" proport.numbase="2"/>
              <staffdef n="2" clef.line="2" clef.shape="G"/>
              <staffdef n="3" clef.line="4" clef.shape="F"/>
              <staffdef n="4" clef.line="4" clef.shape="F"/>
            </staffgrp>
          </scoredef>
          <section>
            <measure n="1">
              <staff n="1">
                <layer n="1">
                  <!-- Must the notes on staff 1 be encoded according to the time sig given
                    in scoredef? That is, without the tuplet? -->
                  <tuplet>
                    <rest dur="8"/>
                    <rest/>
                    <note dur="8" pname="e" oct="5"/>
                  </tuplet>
                </layer>
              </staff>
              <staff n="2">
                <layer n="1">
                  <rest dur="4"/>
                </layer>
              </staff>
            </measure>
            <measure n="2" left="rptstart">
              <staff n="1">
                <layer n="1">
                  <tuplet>
                    <note dur="8" pname="e" oct="5"/>
                    <note pname="f"/>
                    <note pname="g"/>
                  </tuplet>
                </layer>
              </staff>
            </measure>
          </section>
        </score>
      </mdiv>
    </body>
  </music>
</mei>
