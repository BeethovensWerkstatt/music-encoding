    <optional>
      <attribute name="dur">
        <a:documentation xml:lang="eng">encodes logical; that is, written,
          duration.</a:documentation>
        <ref name="data.DURATION.MENSURAL"/>
      </attribute>
    </optional>
    <optional>
      <attribute name="tuplet">
        <a:documentation xml:lang="eng">indicates that this element participates
          in a tuplet.</a:documentation>
        <ref name="data.TUPLET"/>
      </attribute>
    </optional>
