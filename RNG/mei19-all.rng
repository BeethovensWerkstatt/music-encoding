  </define>
  <define name="att.log.rdg">
    <a:documentation xml:lang="eng">Logical domain attributes</a:documentation>
    <optional>
      <attribute name="expan">
        <a:documentation xml:lang="eng">contains an ordered list of IDs of
          descendant &lt;section&gt;, &lt;ending&gt; or
          &lt;rdg&gt; elements. This 'expansion list' indicates how the
          section should be expanded into its 'through-composed' form. For
          example, expan="A End1 A End2" indicates that the section labelled 'A'
          comes first, then the ending labelled 'End1', followed by the 'A'
          section again, and finally the ending labelled
          'End2'.</a:documentation>
        <data type="IDREFS"/>
      </attribute>
    </optional>
  </define>
  <define name="att.vis.rdg">
    <a:documentation xml:lang="eng">Visual domain attributes</a:documentation>
    <empty/>
  </define>

  <!-- ************************************************************** -->
