  <define name="att.vis.sb">
    <a:documentation xml:lang="eng">Visual domain attributes</a:documentation>
    <optional>
      <attribute name="rend">
        <a:documentation xml:lang="eng">indicates whether hash marks should be
          rendered between systems. See Read, p. 436, ex.
          26-3.</a:documentation>
        <choice>
          <value>hash</value>
        </choice>
      </attribute>
